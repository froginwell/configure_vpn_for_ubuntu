[connection]
id=YOUR_NAME
uuid=YOUR_UUID
type=vpn
autoconnect=false
permissions=user:USER_LOGINED:;
secondaries=

[vpn]
gateway=YOUR_IP
require-mppe=yes
user=VPN_USER
password-flags=0
service-type=org.freedesktop.NetworkManager.pptp

[vpn-secrets]
password=VPN_PASSWORD

[ipv4]
dns-search=
method=auto

[ipv6]
dns-search=
method=auto
